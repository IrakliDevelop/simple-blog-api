{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "off",
        "func-names": "off",
        "indent": ["error", 4],
        "no-underscore-dangle": "off",
        "arrow-parens": "off",
        "arrow-body-style": "off"
    }
}
